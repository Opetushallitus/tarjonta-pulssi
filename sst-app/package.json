{
  "name": "tarjonta-pulssi",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "sst dev",
    "build": "sst build",
    "deploy": "sst deploy",
    "remove": "sst remove",
    "console": "sst console",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "prettier": "prettier . --check -u",
    "prettier:fix": "prettier . --write -u",
    "test": "jest",
    "umzug": "ts-node --esm ./packages/shared/db/umzug/migrate.ts",
    "database": "./test/tools/start_postgresql && npm run umzug up",
    "import-db-dumps": "./test/tools/import_db_dumps.sh",
    "prepare-test-env": "npm run database && npm run import-db-dumps",
    "prepare": "cd .. && husky install"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.427.0",
    "@elastic/elasticsearch": "^7.17.0",
    "pg": "^8.11.3",
    "umzug": "^3.3.1"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.3",
    "@types/aws-lambda": "^8.10.126",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "aws-cdk-lib": "2.101.1",
    "constructs": "10.2.69",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-i": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "sst": "2.32.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "workspaces": [
    "packages/*"
  ],
  "lint-staged": {
    "**/*.{js,ts,cjs,jsx,tsx}": [
      "eslint --fix",
      "prettier --write -u"
    ],
    "!**/*.{js,ts,cjs,jsx,tsx}": "prettier --write -u"
  }
}
